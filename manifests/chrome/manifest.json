{
    "manifest_version": 3,
    "name": "IRIS Migration Checklist",
    "version": "1.1.1",
    "description": "Compare migration framework exports between IRIS/Cache servers.",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/api/v1/migration/*",
        "http://*/api/v1/migration/*",
        "https://*/api/atelier/*",
        "http://*/api/atelier/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "96": "icons/icon96.png",
        "128": "icons/icon128.png"
    }
}